{
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jquery": true,
      "jest": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
      "sourceType": "module"
  },
  "globals": {
      "velocity": true,
      "_": true,
      "axios": true,
      "YT": true,
      "anime": true,
      "location": true
  },
  "rules": {
      "prettier/prettier": [
          "error",
          {
              "singleQuote": true,
              "trailingComma": "es5",
              "tabWidth": 2
          }
      ]
  }
}
